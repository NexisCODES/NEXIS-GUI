        end
    )
    L_80_.KeyDown:Connect(
        function(L_102_arg0)
            if L_102_arg0 ~= _G.KEY then
                return
            end
            if L_78_ then
                L_78_ = false
                TextLabel.TextColor3 = Color3.fromRGB(255, 20, 75)
                TextLabel.Text = "------"
            else
                L_78_ = true
