        f.addesp = function()
            pcall(function()
            --print("ESP ran")
            if espforlder then
                espforlder:Destroy()
                espforlder = Instance.new("Folder")
                espforlder.Parent = game.Workspace.CurrentCamera
            else
                espforlder = Instance.new("Folder")
                espforlder.Parent = game.Workspace.CurrentCamera
